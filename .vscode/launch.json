{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: MyArtifact",
            "type": "debugpy",
            "request": "launch",
            "program": "deps.py",
            "args": [
                "-f", "myartifact/pom.xml",
                "-pl", "myartifact",
                "--deps", "junit:junit:jar:4.13.2:test"
            ], 
            "console": "integratedTerminal"
        },
        {
            "name": "Python Debugger: Jarvis",
            "type": "debugpy",
            "request": "launch",
            "program": "deps.py",
            "args": [
                "~/work/raven2/bug-json-parser/Jarvis/crr/pom.xml",
                "-pl", "crr-compute-ecl",
            ], 
            "console": "integratedTerminal"
        },
        {
            "name": "Python Debugger: Starter-Kit",
            "type": "debugpy",
            "request": "launch",
            "program": "deps.py",
            "args": [
                "-f", "~/work/ocf/starter-kit",
                "-pl", "framework",
                "--deps", "org.brotli:dec",
                "-s", "tree"
            ], 
            "console": "integratedTerminal"
        },
        {
            "name": "Python Debugger: LTV",
            "type": "debugpy",
            "request": "launch",
            "program": "deps.py",
            "args": [
                "~/work/LTV/marvel-ltv/pom.xml",
                "-pl", "marvel-ltv-upload-lambda",
                "--deps", "org.mockito:mockito-core:jar:3.6.28:test", "-v", "--color=always"
            ], 
            "console": "integratedTerminal"
        },
        {
            "name": "Python Debugger: pom X",
            "type": "debugpy",
            "request": "launch",
            "program": "deps.py",
            "args": [
                "-f", "tests/pom13.xml"
            ], 
            "console": "integratedTerminal"
        },
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        }

    ]
}